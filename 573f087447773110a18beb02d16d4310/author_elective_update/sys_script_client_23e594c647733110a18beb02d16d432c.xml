        <isolate_script>true</isolate_script>
        <messages/>
        <name>SetISBNDisplayFormat</name>
        <order/>
        <script><![CDATA[function onLoad() {
   if (g_form.isNewRecord() || g_form.isReadOnly()) {
        return; // No need to modify the display format for new records or read-only view
    }

    var isbnField = g_form.getControl('isbn'); // Replace with your actual field name
    if (isbnField) {
        var rawValue = g_form.getValue('isbn'); // Replace with your actual field name
        g_form.setValue('isbn', rawValue); // Set the field value without formatting
    }
}]]></script>
